extends layout

block content
	.w3-container
		.w3-card-4
			header.w3-container.w3-blue
				h3= title
			.w3-container.w3-light-grey
				table.w3-table-all
					tr.w3-blue
						th Codigo
						th Titulo
						each item, index in classes
							tr
								td
									a(href="./"+item.codigo)= item.codigo
								td= item.titulo
			footer.w3-container.w3-light-grey
				address Copyright @ Luis Bouca - PG38933 DWeb2018